{
    "version": 2,
    "build": {
      "env": {
        "NODE_ENV": "production"
      }
    },
    "routes": [
      {
        "src": "/404",
        "dest": "/404.html",
        "status": 404
      },
      {
        "src": "/(.*)",
        "dest": "/index.html",
        "status": 200
      }
    ]
  }